{
  "name": "react-redux-zustand",
  "description": "Showing YouTube videos with Redux and Zustand",
  "keywords": ["React","Redux","Zustand","TypeScript"],
  "private": false,
  "license": "MIT",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
